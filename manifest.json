{
  "manifest_version": 2,

  "name": "Hue Reminder",
  "description": "This extension can give you a prompt when your sql task finished",
  "version": "1.0",

  "browser_action": {
    "default_icon": "logo.png",
	"default_title": "使用时先按F12打开开发者工具",
	"default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
	"tabs",
	"notifications",
	"alarms",
    "http://10.8.8.21:8888/"
  ],
  "devtools_page": "devtools.html",
  "background": { 
	"scripts": ["background.js"],
	"persistent": true
  }
}